{
  "name": "opsvantage-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "postbuild": "node ./scripts/sentry-postbuild.js || true",
    "start": "next start -p 3000",
    "deploy": "vercel --prod",
    "studio": "cd studio && npm run dev",
    "studio:build": "cd studio && npm run build",
    "studio:deploy": "cd studio && npm run deploy",
    "cleanup:lighthouse:ps1": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/cleanup-lighthouse.ps1",
    "cleanup:lighthouse:sh": "bash ./scripts/cleanup-lighthouse.sh",
    "cleanup:lighthouse": "npm run cleanup:lighthouse:ps1 || npm run cleanup:lighthouse:sh"
  },
  "dependencies": {
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@radix-ui/react-slot": "^1.0.6",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "8.13.9",
    "@sentry/nextjs": "^8.55.0",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "framer-motion": "10.12.16",
    "lucide-react": "^0.268.0",
    "next": "^15.3.3",
    "postcss": "^8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.4.7",
    "tar": "^7.5.7",
    "three": "0.165.0",
    "three-mesh-bvh": "^0.8.0"
  },
